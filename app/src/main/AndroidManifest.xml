<manifest xmlns:tools="http://schemas.android.com/tools"
    package="cn.wandersnail.commonsdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_PRIVILEGED_PHONE_STATE"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name="cn.wandersnail.commonsdemo.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="cn.wandersnail.commonsdemo.MainActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="cn.wandersnail.commonsdemo.StorageActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.ClickRippleActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.MD5Activity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.ShareActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.NetStateActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.ConnectWifiActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.ZipActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.ToastUtilsActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.SysFileChooserActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.DebugJudgeActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.ApkDownloadActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.FileOperateActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.TestObserverActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.wandersnail.commonsdemo.DeviceInfoActivity"
            android:configChanges="keyboardHidden|orientation|navigation|screenSize|keyboard"
            android:screenOrientation="portrait"/>
        <activity android:name=".CropActivity" />
        <activity android:name=".TestBack1Activity" />
        <activity android:name=".TestBack2Activity" />
    </application>
</manifest>
